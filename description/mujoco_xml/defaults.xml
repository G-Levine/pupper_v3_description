    <option cone="elliptic" impratio="100" />
    <default>
        <general forcerange="-3 3" forcelimited="true" biastype="affine" gainprm="1 0 0"
            biasprm="0 0 0" />

        <!-- Could make a foot class with these settings and use harder contact for structure -->
